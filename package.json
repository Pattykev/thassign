{
  "name": "thassign",
  "version": "0.0.0",
  "scripts": {
    "android": "tns run android --no-hmr",
    "ios": "tns run ios --no-hmr",
    "mobile": "tns run",
    "preview": "tns preview",
    "clean": "npx rimraf hooks node_modules package-lock.json platforms",
    "ngcc": "ngcc --properties es2015 module main --first-only",
    "postinstall": "npm run ngcc",
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --port 8080 --disable-host-check true --poll=2000",
    "build": "ng build",
    "test": "jest",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test:watch": "jest auth.service.spec.ts --watch"
  },
  "author": "Fotso Teketchuet Stephane",
  "private": true,
  "node": {
    "net": "empty",
    "tls": "empty",
    "dns": "empty"
  },
  "dependencies": {
    "@angular/animations": "~12.2.2",
    "@angular/cdk": "^12.2.2",
    "@angular/common": "~12.2.2",
    "@angular/compiler": "~12.2.2",
    "@angular/core": "~12.2.2",
    "@angular/fire": "^6.1.4",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "~12.2.2",
    "@angular/material": "^12.2.2",
    "@angular/platform-browser": "~12.2.2",
    "@angular/platform-browser-dynamic": "~12.2.2",
    "@angular/router": "~12.2.2",
    "@fortawesome/angular-fontawesome": "^0.8.1",
    "@nativescript/angular": "^11.0.1",
    "@nativescript/core": "^7.1.4",
    "@nativescript/theme": "^3.0.1",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "class-validator": "^0.13.1",
    "core-js": "^3.8.3",
    "epubjs": "^0.3.88",
    "firebase": "^8.2.4",
    "luxon": "^1.25.0",
    "messageformat": "^2.3.0",
    "nativescript-plugin-firebase": "^10.6.3",
    "nativescript-pro-ui": "^3.4.1",
    "ngx-translate-messageformat-compiler": "^4.9.0",
    "reflect-metadata": "~0.1.13",
    "rxjs": "^6.6.3",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1202.2",
    "@angular-devkit/build-angular": "~12.2.2",
    "@angular/cli": "~12.2.2",
    "@angular/compiler-cli": "~12.2.2",
    "@angular/language-service": "~12.2.2",
    "@nativescript/schematics": "~11.0.0",
    "@nativescript/tslint-rules": "~0.0.5",
    "@nativescript/types": "^7.1.0",
    "@nativescript/webpack": "~4.0.1",
    "@ngneat/spectator": "^6.1.3",
    "@ngtools/webpack": "^11.1.1",
    "@types/faker": "^5.1.5",
    "@types/jest": "^26.0.20",
    "@types/luxon": "^1.25.1",
    "@types/node": "^14.14.22",
    "codelyzer": "^6.0.1",
    "copy-webpack-plugin": "^7.0.0",
    "faker": "^5.2.0",
    "firebase-tools": "^9.2.2",
    "fuzzy": "^0.1.3",
    "inquirer": "^7.3.3",
    "inquirer-autocomplete-prompt": "^1.3.0",
    "jest": "^26.6.3",
    "jest-preset-angular": "8.3.2",
    "open": "^7.3.1",
    "protractor": "~7.0.0",
    "ts-jest": "^26.4.4",
    "ts-node": "~9.1.1",
    "tslint": "^6.1.0",
    "typescript": "~4.3.5"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "roots": [
      "src"
    ],
    "transform": {
      "^.+\\.(ts|js|html)$": "ts-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/setup-jest.ts"
    ],
    "collectCoverage": true,
    "moduleNameMapper": {
      "@app/(.*)": "<rootDir>/src/app/$1",
      "@assets/(.*)": "<rootDir>/src/assets/$1",
      "@core/(.*)": "<rootDir>/src/app/core/$1",
      "@env": "<rootDir>/src/environments/environment",
      "@src/(.*)": "<rootDir>/src/src/$1",
      "@state/(.*)": "<rootDir>/src/app/state/$1"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/tsconfig.spec.json",
        "stringifyContentPathRegex": "\\.html$",
        "astTransformers": [
          "jest-preset-angular/build/InlineFilesTransformer",
          "jest-preset-angular/build/StripStylesTransformer"
        ]
      }
    }
  },
  "nativescript": {
    "id": "com.lesauf.thassign",
    "tns-android": {
      "version": "6.5.3"
    }
  }
}
