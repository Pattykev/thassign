{
  "name": "thassign",
  "version": "0.0.0",
  "scripts": {
    "android": "tns run android --no-hmr",
    "ios": "tns run ios --no-hmr",
    "mobile": "tns run",
    "preview": "tns preview",
    "clean": "npx rimraf hooks node_modules package-lock.json platforms",
    "ngcc": "ngcc --properties es2015 module main --first-only",
    "postinstall": "npm run ngcc",
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --port 8080 --disableHostCheck true --poll=2000",
    "build": "ng build",
    "test": "jest",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test:watch": "jest auth.service.spec.ts --watch"
  },
  "author": "Fotso Teketchuet Stephane",
  "private": true,
  "node": {
    "net": "empty",
    "tls": "empty",
    "dns": "empty"
  },
  "dependencies": {
    "@angular/animations": "~10.0.11",
    "@angular/cdk": "^10.1.3",
    "@angular/common": "~10.0.11",
    "@angular/compiler": "~10.0.11",
    "@angular/core": "~10.0.11",
    "@angular/fire": "^6.0.2",
    "@angular/flex-layout": "^10.0.0-beta.32",
    "@angular/forms": "~10.0.11",
    "@angular/material": "^10.1.3",
    "@angular/platform-browser": "~10.0.11",
    "@angular/platform-browser-dynamic": "~10.0.11",
    "@angular/router": "~10.0.11",
    "@fortawesome/angular-fontawesome": "^0.7.0",
    "@nativescript/angular": "~10.0.0",
    "@nativescript/core": "rc",
    "@nativescript/theme": "^2.2.1",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^4.0.0",
    "class-validator": "^0.11.1",
    "core-js": "^2.6.9",
    "firebase": "^7.13.1",
    "luxon": "^1.24.1",
    "messageformat": "^2.3.0",
    "mongodb-stitch-browser-sdk": "^4.8.0",
    "mongodb-stitch-browser-services-http": "^4.8.0",
    "nativescript-plugin-firebase": "^10.5.2",
    "nativescript-pro-ui": "^3.4.1",
    "ngx-translate-messageformat-compiler": "^4.6.0",
    "realm-web": "^0.8.1",
    "reflect-metadata": "~0.1.12",
    "rxjs": "^6.5.5",
    "tslib": "1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.2",
    "@angular/cli": "~10.0.6",
    "@angular/compiler-cli": "~10.0.11",
    "@angular/language-service": "~10.0.11",
    "@nativescript/schematics": "~9.0.0",
    "@nativescript/tslint-rules": "~0.0.5",
    "@nativescript/types": "rc",
    "@nativescript/webpack": "~2.0.0",
    "@ngneat/spectator": "^5.3.1",
    "@ngtools/webpack": "~10.0.0",
    "@types/faker": "^4.1.11",
    "@types/jest": "^25.2.1",
    "@types/luxon": "^1.22.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "copy-webpack-plugin": "^6.0.3",
    "faker": "^4.1.0",
    "jest": "^25.5.4",
    "jest-preset-angular": "8.1.3",
    "mongodb": "^3.6.0",
    "protractor": "~5.4.3",
    "ts-jest": "^25.5.1",
    "ts-node": "~8.3.0",
    "tslint": "^6.1.0",
    "typescript": "~3.9.0",
    "@angular-devkit/architect": ">= 0.900 < 0.1100",
    "firebase-tools": "^8.0.0",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "open": "^7.0.3"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "roots": [
      "src"
    ],
    "transform": {
      "^.+\\.(ts|js|html)$": "ts-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/setup-jest.ts"
    ],
    "collectCoverage": true,
    "moduleNameMapper": {
      "@app/(.*)": "<rootDir>/src/app/$1",
      "@assets/(.*)": "<rootDir>/src/assets/$1",
      "@core/(.*)": "<rootDir>/src/app/core/$1",
      "@env": "<rootDir>/src/environments/environment",
      "@src/(.*)": "<rootDir>/src/src/$1",
      "@state/(.*)": "<rootDir>/src/app/state/$1"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/tsconfig.spec.json",
        "stringifyContentPathRegex": "\\.html$",
        "astTransformers": [
          "jest-preset-angular/build/InlineFilesTransformer",
          "jest-preset-angular/build/StripStylesTransformer"
        ]
      }
    }
  },
  "nativescript": {
    "id": "com.lesauf.thassign",
    "tns-android": {
      "version": "6.5.3"
    }
  }
}
