{
  "name": "thassign",
  "version": "0.0.0",
  "scripts": {
    "android": "tns run android --no-hmr",
    "ios": "tns run ios --no-hmr",
    "mobile": "tns run",
    "preview": "tns preview",
    "ngcc": "ngcc --properties es2015 module main --first-only",
    "postinstall": "npm run ngcc",
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --port 8080 --disableHostCheck true --poll=2000",
    "build": "ng build",
    "test": "jest",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test:watch": "jest auth.service.spec.ts --watch"
  },
  "author": "Fotso Teketchuet Stephane",
  "private": true,
  "node": {
    "net": "empty",
    "tls": "empty",
    "dns": "empty"
  },
  "dependencies": {
    "@angular/animations": "~9.1.9",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~9.1.9",
    "@angular/compiler": "~9.1.9",
    "@angular/core": "~9.1.9",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "~9.1.9",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "~9.1.9",
    "@angular/platform-browser-dynamic": "~9.1.9",
    "@angular/router": "~9.1.9",
    "@nativescript/angular": "~9.0.0",
    "@nativescript/core": "^6.5.15",
    "@nativescript/theme": "^2.3.3",
    "@ngx-translate/core": "^12.1.2",
    "@ngx-translate/http-loader": "^4.0.0",
    "class-validator": "^0.11.1",
    "luxon": "^1.23.0",
    "messageformat": "^2.3.0",
    "mongodb-stitch-browser-sdk": "^4.8.0",
    "mongodb-stitch-browser-services-http": "^4.8.0",
    "ngx-translate-messageformat-compiler": "^4.6.0",
    "reflect-metadata": "~0.1.12",
    "rxjs": "^6.5.5",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.7",
    "@angular/cli": "~9.1.7",
    "@angular/compiler-cli": "~9.1.9",
    "@angular/language-service": "~9.1.9",
    "@nativescript/schematics": "~9.0.0",
    "@nativescript/tslint-rules": "~0.0.5",
    "@nativescript/webpack": "~2.0.0",
    "@ngneat/spectator": "^5.3.1",
    "@ngtools/webpack": "~9.1.0",
    "@types/faker": "^4.1.11",
    "@types/jest": "^25.2.1",
    "@types/luxon": "^1.22.0",
    "@types/node": "^13.11.0",
    "codelyzer": "^5.1.2",
    "faker": "^4.1.0",
    "jest": "^25.2.7",
    "jest-preset-angular": "8.1.3",
    "mongodb": "^3.5.8",
    "protractor": "~5.4.3",
    "ts-jest": "^25.3.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "roots": [
      "src"
    ],
    "transform": {
      "^.+\\.(ts|js|html)$": "ts-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/setup-jest.ts"
    ],
    "collectCoverage": true,
    "moduleNameMapper": {
      "@app/(.*)": "<rootDir>/src/app/$1",
      "@assets/(.*)": "<rootDir>/src/assets/$1",
      "@core/(.*)": "<rootDir>/src/app/core/$1",
      "@env": "<rootDir>/src/environments/environment",
      "@src/(.*)": "<rootDir>/src/src/$1",
      "@state/(.*)": "<rootDir>/src/app/state/$1"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/tsconfig.spec.json",
        "stringifyContentPathRegex": "\\.html$",
        "astTransformers": [
          "jest-preset-angular/build/InlineFilesTransformer",
          "jest-preset-angular/build/StripStylesTransformer"
        ]
      }
    }
  },
  "nativescript": {
    "id": "org.lesauf.thassign",
    "tns-android": {
      "version": "6.5.3"
    }
  }
}
